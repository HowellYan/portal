<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:annotation />

	<dubbo:application name="bppe_portal_resources" />
	<!--1.0zk -->
	<dubbo:registry id="zk1.0" protocol="zookeeper" register="false" address="172.26.6.60:2181" />
	<!-- 2.0zk -->
	<dubbo:registry id="zk2.0" protocol="zookeeper" register="false" address="172.26.7.23:2181" />
	
	<!--消费者 -->
	<dubbo:consumer filter="dubboLogFilter" check="false" timeout="70000" retries="0" />


</beans>
